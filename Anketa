<script>
  // Build scale buttons
  const scales = [
    { id: 'scale_start', name: 'scale_start' },
    { id: 'scale_currencies', name: 'scale_currencies' },
    { id: 'scale_orders', name: 'scale_orders' },
    { id: 'scale_channel', name: 'scale_channel' },
    { id: 'scale_roles', name: 'scale_roles' },
    { id: 'scale_helpful', name: 'scale_helpful' },
    { id: 'scale_bot_ux', name: 'scale_bot_ux' },
    { id: 'scale_nps', name: 'scale_nps' },
  ];

  scales.forEach(({ id, name }) => {
    const container = document.getElementById(id);
    for (let i = 1; i <= 5; i++) {
      const btn = document.createElement('button');
      btn.type = 'button';
      btn.className = 'scale-btn';
      btn.textContent = i;
      btn.dataset.value = i;
      btn.dataset.group = name;
      btn.addEventListener('click', () => {
        container.querySelectorAll('.scale-btn').forEach(b => b.classList.remove('selected'));
        btn.classList.add('selected');
        btn.dataset.selected = 'true';
        updateProgress();
      });
      container.appendChild(btn);
    }
  });

  // Progress tracking
  function updateProgress() {
    const radioNames = new Set();
    document.querySelectorAll('input[type="radio"]').forEach(r => radioNames.add(r.name));

    let total = radioNames.size + scales.length;
    let filled = 0;

    radioNames.forEach(name => {
      if (document.querySelector(`input[name="${name}"]:checked`)) filled++;
    });

    scales.forEach(({ id }) => {
      if (document.querySelector(`#${id} .scale-btn.selected`)) filled++;
    });

    const pct = Math.round((filled / total) * 100);
    document.getElementById('progress').style.width = pct + '%';
  }

  document.querySelectorAll('input[type="radio"], input[type="checkbox"]').forEach(el => {
    el.addEventListener('change', updateProgress);
  });

  // ‚ñº –í—Å—Ç–∞–≤—å—Ç–µ —Å—é–¥–∞ –≤–∞—à—É —Å—Å—ã–ª–∫—É –ø–æ—Å–ª–µ –ø—É–±–ª–∏–∫–∞—Ü–∏–∏ Apps Script
  const SCRIPT_URL = 'https://script.google.com/macros/s/AKfycbzlLTI6j_kjFpSvowG7gUZIEdgH7ZC8j9peAIrbwv0R1v-QX8LX1JsVMRrvKzl1zJw/exec';

  document.getElementById('survey').addEventListener('submit', async function(e) {
    e.preventDefault();

    const btn = this.querySelector('.submit-btn');
    btn.textContent = '–û—Ç–ø—Ä–∞–≤–ª—è–µ–º...';
    btn.disabled = true;

    // –°–æ–±–∏—Ä–∞–µ–º –≤—Å–µ –ø–æ–ª—è —Ñ–æ—Ä–º—ã
    const fd = new FormData(this);
    const data = {};

    // Radio –∏ text/textarea ‚Äî –ø—Ä–æ—Å—Ç—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è
    for (const [key, val] of fd.entries()) {
      if (data[key]) {
        // checkbox —Å –Ω–µ—Å–∫–æ–ª—å–∫–∏–º–∏ –∑–Ω–∞—á–µ–Ω–∏—è–º–∏ ‚Üí –º–∞—Å—Å–∏–≤
        data[key] = [].concat(data[key], val);
      } else {
        data[key] = val;
      }
    }

    // Scale –∫–Ω–æ–ø–∫–∏ (–æ–Ω–∏ –Ω–µ –≤ FormData, —Ç.–∫. —ç—Ç–æ <button>, –Ω–µ <input>)
    document.querySelectorAll('.scale-btn.selected').forEach(btn => {
      data[btn.dataset.group] = btn.dataset.value;
    });

    try {
      await fetch(SCRIPT_URL, {
        method: 'POST',
        mode: 'no-cors', // Google Apps Script —Ç—Ä–µ–±—É–µ—Ç no-cors
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify(data),
      });
    } catch (err) {
      // no-cors –≤—Å–µ–≥–¥–∞ –≤–æ–∑–≤—Ä–∞—â–∞–µ—Ç opaque response, –æ—à–∏–±–∫–∏ –∑–¥–µ—Å—å —Ä–µ–¥–∫–∏
      console.warn('fetch error:', err);
    }

    // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —ç–∫—Ä–∞–Ω —É—Å–ø–µ—Ö–∞ –≤ –ª—é–±–æ–º —Å–ª—É—á–∞–µ
    this.style.display = 'none';
    document.getElementById('success').style.display = 'block';
    window.scrollTo({ top: 0, behavior: 'smooth' });
  });
</script>
</body>
</html>
